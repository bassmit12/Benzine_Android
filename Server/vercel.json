{
  "version": 2,
  "builds": [
    {
      "src": "src/**",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/src/$1" }],
  "functions": {
    "api/**/*.js": {
      "runtime": "@now/node-server"
    }
  }
}
